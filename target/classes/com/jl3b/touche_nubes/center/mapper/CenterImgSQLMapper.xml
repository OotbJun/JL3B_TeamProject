<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" 
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.jl3b.touche_nubes.center.mapper.CenterImgSQLMapper">
	
	<insert id="insertCenterInfoImg">
		<![CDATA[
			INSERT INTO JL3B_CENTER_IMG 
			VALUES(
				JL3B_CENTER_IMG_SEQ.NEXTVAL, 
				#{info_no}, 
				#{center_img_title}, 
				#{center_img_path}, 
				SYSDATE
			)
		]]>
	</insert>
	
	<select id="selectCenterInfoImg" resultType="com.jl3b.touche_nubes.centervo.CenterImgVo">
		<![CDATA[
			SELECT * 
           	FROM 
           		JL3B_CENTER_IMG 
           	WHERE info_no = #{info_no} 
		]]>	
	</select>
	
	<select id="test" resultType="com.jl3b.touche_nubes.centervo.CenterImgVo">
		<![CDATA[
			SELECT * 
           	FROM 
           		JL3B_CENTER_IMG 
           	WHERE info_no = #{info_no} 
		]]>
	</select>
	
</mapper>